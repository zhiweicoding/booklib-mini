<block wx:if="{{$taroCompReady}}">
    <view class="{{rootCls}}" style="{{anonymousState__1}}">
        <view class="{{minusBtnCls}}" bindtap="handleClick" data-e-tap-so="this" data-e-tap-a-a="minus"><text class="at-icon at-icon-subtract at-input-number__btn-subtract"></text>
        </view>
        <input class="at-input-number__input" style="{{anonymousState__2}}" type="{{type}}" value="{{inputValue}}" disabled="{{disabledInput || disabled}}" bindinput="handleInput" bindblur="handleBlur" />
        <view class="{{plusBtnCls}}" bindtap="handleClick" data-e-tap-so="this" data-e-tap-a-a="plus"><text class="at-icon at-icon-add at-input-number__btn-add"></text>
        </view>
    </view>
</block>