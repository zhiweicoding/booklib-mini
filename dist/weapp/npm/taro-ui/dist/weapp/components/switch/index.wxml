<block wx:if="{{$taroCompReady}}">
    <view class="{{rootCls}}" style="{{anonymousState__1}}">
        <view class="at-switch__title">{{title}}</view>
        <view class="{{containerCls}}">
            <view class="at-switch__mask"></view>
            <switch class="at-switch__switch" checked="{{checked}}" color="{{color}}" bindchange="handleChange"></switch>
        </view>
    </view>
</block>