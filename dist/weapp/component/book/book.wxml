<block wx:if="{{$taroCompReady}}">
    <view class="bookItem ">
        <view class="bookItem-left" style="position:relative;" bindtap="openUrl" data-e-tap-so="this" data-e-tap-a-a="{{item}}" data-e-tap-a-b="{{whereFrom}}">
            <image class="bookItem-left-img" src="https://lg-qrm18qcm-1255940368.cos.ap-shanghai.myqcloud.com/miniProgram/v2-7f1b40d87168cda877a05d9ff55007b6_720w.jpeg" background-size="cover" mode="aspectFill" lazy-load="{{true}}"></image>
        </view>
        <view class="bookItem-right" bindtap="openUrl" data-e-tap-so="this" data-e-tap-a-a="{{item}}" data-e-tap-a-b="{{whereFrom}}">
            <view class="bookItem-right-title">
                <view class="bookItem-right-title-name">天津年鉴</view>
                <view class="bookItem-right-title-year">2021</view>
            </view>
            <view class="bookItem-right-three">天津市地方志编修委员会</view>
            <view class="bookItem-right-four">《天津年鉴》编辑部</view>
            <view class="bookItem-right-read">
                <view class="bookItem-right-read-title">阅读量：</view>
                <view class="bookItem-right-read-number">22990</view>
            </view>
        </view>
    </view>
</block>